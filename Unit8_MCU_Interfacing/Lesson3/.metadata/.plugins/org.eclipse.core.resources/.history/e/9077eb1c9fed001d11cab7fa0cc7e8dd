void delay_ms(uint32_t n){
	int i, j;
	for(i = 0; i < n; i++)
		for(j = 0; j < 255; j++);
}

void Clock_init(){
	RCC_GPIOA_CLK_ENABLE();
	RCC_GPIOB_CLK_ENABLE();
	RCC_AFIO_CLK_ENABLE();
}

